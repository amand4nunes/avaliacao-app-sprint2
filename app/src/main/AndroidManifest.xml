<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vamos_lucrar">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/agenda"
        android:label="@string/app_name"
        android:roundIcon="@drawable/agenda"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.ListaContatosActivity"></activity>
        <activity android:name=".views.DetalhesActivity" />
        <activity android:name=".views.CadastroActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>